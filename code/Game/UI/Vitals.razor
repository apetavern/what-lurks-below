@using System
@using Sandbox;
@using Sandbox.UI;
@inherits Panel
@attribute [StyleSheet]

<root>
    <label>@Health HP</label>
</root>

@code
{
    public static Vitals Instance;
    public string Health { get; set; }

    public Vitals()
    {
        Instance = this;
    }

    protected override int BuildHash()
    {
        return HashCode.Combine(Health);
    }
}
